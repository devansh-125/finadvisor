{
  "typescript.validate.enable": false,
  "typescript.enablePromptUseWorkspaceTsdk": false,
  "typescript.check.skipLibCheck": true,
  "typescript.tsserver.skipLibCheck": true,
  "files.exclude": {
    "**/node_modules": true,
    "**/.git": true,
    "**/.vscode": true,
    "**/tsconfig.json": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/.git": true,
    "dist": true,
    "build": true
  },
  "[javascript]": {
    "editor.defaultFormatter": null
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
